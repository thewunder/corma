{
  "name": "thewunder/corma",
  "description": "Convention-based Alternative ORM",
  "minimum-stability": "stable",
  "type": "library",
  "keywords": ["Object Relational Mapper", "Data Mapper"],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael O'Connell",
      "email": "wundbread@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Corma\\" : "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Corma\\Test\\": "test/"}
  },
  "require": {
    "php": ">=8.1",
    "ext-pdo" : "*",
    "ext-json": "*",
    "doctrine/inflector": "^1.3||^2.0",
    "psr/event-dispatcher": "^1.0",
    "psr/simple-cache": "^2.0||^3.0",
    "psr/container": "^2.0",
    "thewunder/corma-dbal": "1.0.x-dev"
  },
  "require-dev": {
    "vlucas/phpdotenv": "~2.0",
    "phpunit/phpunit": "^10.0",
    "symfony/event-dispatcher": "^6.0 || ^7.0",
    "rector/rector": "^1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "suggest": {
    "robmorgan/phinx": "*"
  }
}
